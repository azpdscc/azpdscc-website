rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow public read and write access for everyone.
    // IMPORTANT: This is insecure and for development purposes only.
    // Follow the instructions in SECURITY.md to lock this down before launch.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
